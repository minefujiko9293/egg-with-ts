{
  "name": "egg-with-ts",
  "version": "1.0.0",
  "description": "egg with typescript",
  "private": true,
  "egg": {
    "typescript": true,
    "declarations": true
  },
  "scripts": {
    "dev": "egg-bin dev",
    "debug": "egg-bin debug",
    "clean": "tsc -b --clean",
    "lint": "eslint .",
    "cov": "egg-bin cov",
    "tsc": "tsc -p tsconfig.json",
    "ci": "npm run clean && npm run lint && npm run cov && npm run tsc",
    "start": "egg-scripts start --daemon --title=egg-with-ts",
    "stop": "egg-scripts stop --title=egg-with-ts"
  },
  "dependencies": {
    "egg": "^3.11.0",
    "egg-cors": "^3.0.0",
    "egg-redis": "^2.5.0",
    "egg-scripts": "^2.17.0",
    "egg-validate": "^2.0.2",
    "moment": "^2.22.0"
  },
  "devDependencies": {
    "@eggjs/tsconfig": "^1.1.0",
    "@types/cheerio": "^0.22.1",
    "@types/mocha": "^10.0.1",
    "cheerio": "^1.0.0-rc.2",
    "egg-bin": "^5.9.0",
    "egg-mock": "^5.5.0",
    "eslint": "^8.31.0",
    "eslint-config-egg": "^12.1.0",
    "typescript": "^4.9.4"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
